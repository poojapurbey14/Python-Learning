year = int(input("Enter a year: "))
a=year%4
b=year%100
c=year%400
if year>1582:
 if a!=0:
    print(year," is common year")
 elif b!=0:
    print(year,"is leap year")
 elif c!=0:
    print(year,"is common year")
 else:
    print("Its a leap year")
else :
    print("Not within the Gregorian calendar period")

